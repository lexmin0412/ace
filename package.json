{
  "name": "@lexmin0412/ace",
  "version": "0.5.2",
  "type": "module",
  "description": "ACE, means Antd Components Enhanced, a lib with several extreamly enhanced react components based on antd",
  "keywords": [
    "ace",
    "antd-components-enhanced",
    "antd",
    "react-components"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lexmin0412/ace"
  },
  "license": "MIT",
  "author": {
    "name": "lexmin0412",
    "email": "lexmin0412@outlook.com",
    "url": "https://github.com/lexmin0412"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "capture-website": "node ./scripts/capture-website.mjs",
    "build": "rslib build",
    "build:storybook": "storybook build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "storybook": "storybook dev",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "release": "release-it"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "antd": "^5.23.0",
    "json-rules-engine": "^7.3.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@release-it/conventional-changelog": "^10.0.0",
    "@rsbuild/core": "1.2.0-alpha.0",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rslib/core": "^0.3.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@toolkit-fe/clipboard": "^0.1.22",
    "@types/lodash": "^4.17.14",
    "@types/react": "^19.0.2",
    "ahooks": "^3.8.4",
    "jsdom": "^25.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "release-it": "^15.10.0",
    "storybook": "^8.4.7",
    "storybook-addon-rslib": "^0.1.7",
    "storybook-react-rsbuild": "^0.1.7",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16.14.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
